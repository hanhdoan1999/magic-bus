<%@page contentType="text/html; charset=UTF-8"%>
<%@include file="/WEB-INF/pages/common/define.jsp"%>
<jsp:include page="/WEB-INF/pages/common/header.jsp">
	<jsp:param value="Màn hình quản lý đăng nhập"
		name="title_for_header_jsp" />
</jsp:include>
<style>
.formdangki {
	width: 460px;
	padding: 35px 35px;
	margin: 35px 35px;
	background-color: white;
	-webkit-box-shadow: 0 0 20px rgba(0, 0, 0, .1);
	box-shadow: 0 0 20px rgba(0, 0, 0, .1);
}
</style>
<div class="container">
	<div class="col-md-12" style="background-color: #f1eddb;">
		<div class="formdangki">
			<h2>ĐĂNG NHẬP</h2>
			<form action="loginAdminConfirm">
				<div class="form-group">
					<label for="idQuanLy">UserName:</label> <input type="text"
						class="form-control" id="idQuanLy" placeholder="UserName"
						name="idQuanLy" required>
				</div>
				<div class="form-group">
					<label for="matKhau">Password:</label> <input type="password"
						class="form-control" id="matKhau" placeholder="password"
						name="matKhau" required>
				</div>
				<div class="form-group">
					<button type="submit" class="btn btn-danger">Đăng Nhập</button>
				</div>
			</form>
		</div>
	</div>
</div>
<jsp:include page="/WEB-INF/pages/common/footer.jsp" />
